{
  "name": "@onyx-escrow/sdk",
  "version": "1.2.0",
  "description": "TypeScript SDK for Onyx Escrow-as-a-Service (EaaS) API - Monero non-custodial escrow with FROST threshold signing",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./escrow": {
      "types": "./dist/escrow.d.ts",
      "import": "./dist/escrow.mjs",
      "require": "./dist/escrow.js"
    },
    "./webhooks": {
      "types": "./dist/webhooks.d.ts",
      "import": "./dist/webhooks.mjs",
      "require": "./dist/webhooks.js"
    },
    "./frost": {
      "types": "./dist/frost.d.ts",
      "import": "./dist/frost.mjs",
      "require": "./dist/frost.js"
    },
    "./fees": {
      "types": "./dist/fees.d.ts",
      "import": "./dist/fees.mjs",
      "require": "./dist/fees.js"
    },
    "./analytics": {
      "types": "./dist/analytics.d.ts",
      "import": "./dist/analytics.mjs",
      "require": "./dist/analytics.js"
    },
    "./apikeys": {
      "types": "./dist/apikeys.d.ts",
      "import": "./dist/apikeys.mjs",
      "require": "./dist/apikeys.js"
    },
    "./chat": {
      "types": "./dist/chat.d.ts",
      "import": "./dist/chat.mjs",
      "require": "./dist/chat.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "onyx",
    "escrow",
    "monero",
    "xmr",
    "cryptocurrency",
    "eaas",
    "escrow-as-a-service",
    "frost",
    "threshold-signatures",
    "multisig",
    "api-client"
  ],
  "author": "Onyx Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
